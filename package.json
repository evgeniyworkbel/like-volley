{
  "name": "like-volley",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "tsc && next build --turbopack",
    "start": "next start",
    "lint:lang": "eslint .",
    "lint:style": "stylelint \"**/*.css\"",
    "lint": "npm run lint:lang && npm run lint:style",
    "lint:fix": "eslint --fix . && stylelint \"**/*.css\" --fix",
    "format:check": "prettier -c .",
    "format": "prettier -w .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier -w --ignore-unknown"
    ],
    "**/*.{css}": [
      "stylelint \"**/*.css\" --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-input/mask": "^2.0.4",
    "clsx": "^2.1.1",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@evgeniyworkbel/prettier-config": "^1.0.2",
    "@evgeniyworkbel/stylelint-config": "^1.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-useeffect": "^1.0.12",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.23.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
