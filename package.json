{
  "name": "like-volley",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build:compile": "next build --experimental-build-mode compile",
    "build:generateEnv": "next build --experimental-build-mode generate-env",
    "build:generate": "next build --experimental-build-mode generate",
    "build": "next build",
    "start": "next start",
    "lint:lang": "eslint .",
    "lint:style": "stylelint \"**/*.css\"",
    "lint": "pnpm run lint:lang && pnpm run lint:style",
    "lint:fix": "eslint --fix . && stylelint \"**/*.css\" --fix",
    "format:check": "prettier -c .",
    "format": "prettier -w .",
    "prepare": "husky",
    "payload": "payload",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "migrate:create": "payload migrate:create --skip-empty",
    "migrate:status": "payload migrate:status",
    "managed-ci": "payload migrate && pnpm build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier -w --ignore-unknown"
    ],
    "**/*.{css}": [
      "stylelint \"**/*.css\" --fix"
    ]
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-postgres": "3.76.1",
    "@payloadcms/next": "3.76.1",
    "@payloadcms/richtext-lexical": "3.76.1",
    "@payloadcms/translations": "3.76.1",
    "@payloadcms/ui": "3.76.1",
    "@react-input/mask": "^2.0.4",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "next": "16.1.6",
    "payload": "3.76.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@evgeniyworkbel/prettier-config": "^1.0.2",
    "@evgeniyworkbel/stylelint-config": "^1.0.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.27",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@yandex/ymaps3-types": "^1.0.18413413",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-useeffect": "^1.0.12",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.23.1",
    "tailwindcss": "^4.1.18",
    "type-fest": "^5.3.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}